
<section class="ui raised segment">
  <div class="ui grid">
      <table class="ui celled table segment">
        <thead>
          <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Password</th>
            <th>Delete</th>
          </tr>
        </thead>
        <tbody>
          {{#each users}}
            <tr>
              <td>{{firstName}}</td>
              <td>{{lastName}}</td>
              <td>{{email}}</td>
              <td>*****</td>
              <td><button><a id="useradmin" class="negative ui button" href="/useradmin/{{_id}}">Delete</a></button></td>
            </tr>
          {{/each}}
        </tbody>
      </table>
  </div>
</section>
